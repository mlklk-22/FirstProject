<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="keywords"
        content="wrappixel, admin dashboard, html css dashboard, web dashboard, bootstrap 5 admin, bootstrap 5, css3 dashboard, bootstrap 5 dashboard, Ample lite admin bootstrap 5 dashboard, frontend, responsive bootstrap 5 admin template, Ample admin lite dashboard bootstrap 5 dashboard template">
    <meta name="description"
        content="Ample Admin Lite is powerful and clean admin dashboard template, inpired from Bootstrap Framework">
    <meta name="robots" content="noindex,nofollow">
    <title>Ample Admin Lite Template by WrapPixel</title>
    <link rel="canonical" href="https://www.wrappixel.com/templates/ample-admin-lite/" />
  
    <link href="~/DashboardAssests2/plugins/bower_components/chartist/dist/chartist.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/DashboardAssests2/plugins/bower_components/chartist-plugin-tooltips/dist/chartist-plugin-tooltip.css">
    <link href="~/DashboardAssests2/css/style.min.css" rel="stylesheet">
</head>
<body>
      @{
var name = ViewBag.AdminName;
var firstname = ViewBag.FirstName;
var Lastname = ViewBag.LastName;
}
    
    <div id="main-wrapper" data-layout="vertical" data-navbarbg="skin5" data-sidebartype="full"
        data-sidebar-position="absolute" data-header-position="absolute" data-boxed-layout="full">
       
         <header class="topbar" data-navbarbg="skin5">
            <nav class="navbar top-navbar navbar-expand-md navbar-dark">
                <div class="navbar-header" data-logobg="skin6">
                   
                    <a class="navbar-brand">
                       
                        <b class="logo-icon" style="color:black">
                           @firstname
                        </b>
                      
                        
                         <b class="logo-icon" style="color:black">
                            @Lastname
                        </b>
                    </a>
                    
                    <a class="nav-toggler waves-effect waves-light text-dark d-block d-md-none"
                        href="javascript:void(0)"><i class="ti-menu ti-close"></i></a>
                </div>
               
                <div class="navbar-collapse collapse" id="navbarSupportedContent" data-navbarbg="skin5">
                   
                  
                    <ul class="navbar-nav ms-auto d-flex align-items-center">
    
                      
                      @*  <li class=" in">
                            <form role="search" class="app-search d-none d-md-block me-3">
                                <input type="text" placeholder="Search..." class="form-control mt-0">
                                <a href="" class="active">
                                    <i class="fa fa-search"></i>
                                </a>
                            </form>
                        </li>*@
                       
                        <li>
                            <a class="profile-pic" href="#">
                                <img src="~/Assests/images/icons8-male-user-24.png" alt="user-img" width="36"
                                    class="img-circle"><span class="text-white font-medium">@name</span></a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
      
        <aside class="left-sidebar"  data-sidebarbg="skin6">  
            <div class="scroll-sidebar" 
                <nav class="sidebar-nav">
                    <ul id="sidebarnav">
                        <li class="sidebar-item pt-2">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link"  asp-controller="Homef" asp-action="Dashboard" aria-expanded="false">
                                 <i class="fa fa-bar-chart" aria-hidden="true"></i>
                                <span class="hide-menu">Main</span>
                            </a>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link" asp-controller="Logins" asp-action="Index"
                                aria-expanded="false">
                                <i class="fa fa-users" aria-hidden="true"></i>
                                <span class="hide-menu">Users</span>
                            </a>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link" asp-controller="Homef" asp-action="Index"
                                aria-expanded="false">
                                <i class="fa fa-home"  aria-hidden="true"></i>
                                <span class="hide-menu"> Home</span>
                            </a>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link" asp-controller="Aboutus" asp-action="Index"
                                aria-expanded="false">
                                <i class="fa fa-info-circle" aria-hidden="true"></i>
                                <span class="hide-menu"> About Us</span>
                            </a>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link" asp-controller="Contactus" asp-action="Index"
                                aria-expanded="false">
                                <i class="fa fa-phone" aria-hidden="true"></i>
                                <span class="hide-menu">Contact Us</span>
                            </a>
                        </li> 
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link" asp-controller="Testmonials" asp-action="Index"
                                aria-expanded="false">
                                <i class="fa fa-certificate" aria-hidden="true"></i>
                                <span class="hide-menu">Testmonials</span>
                            </a>
                        </li>
                        @*<li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link" asp-controller="Leavemessages" asp-action="Index"
                                aria-expanded="false">
                                <i class="fa fa-filter" aria-hidden="true"></i>
                                <span class="hide-menu"> Suggetions</span>
                            </a>
                        </li>*@
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link" asp-controller="Reservations" asp-action="Index"
                                aria-expanded="false">
                                <i class="fa fa-shopping-bag" aria-hidden="true"></i>
                                <span class="hide-menu"> Reservations</span>
                            </a>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link" asp-controller="Visas" asp-action="Index"
                                aria-expanded="false">
                                <i class="fa fa-credit-card" aria-hidden="true"></i>
                                <span class="hide-menu">Payment Details</span>
                            </a>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link" asp-controller="Visas" asp-action="ReqRes"
                                aria-expanded="false">
                                <i class="fa fa-registered" aria-hidden="true"></i>
                                <span class="hide-menu"> Requsted Reservation</span>
                            </a>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link" href="~/Logins/Edit/@name"
                                aria-expanded="false">
                                <i class="fa fa-male" aria-hidden="true"></i>
                                <span class="hide-menu"> My Profile</span>
                            </a>
                        </li>
                        <li class="text-center p-20 upgrade-btn">
                            <a asp-area="" asp-controller="LoginAndRegistration" asp-action="LogOut"
                                class="btn d-grid  text-white" target="_self" style="background-color:#094c5b">
                                Sign Out</a>
                        </li>
                        
                        
                    </ul>

                </nav>
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
        </aside>
        <br /><br /><br />
    <div  style="margin-left:350px; width:70%">
        @RenderBody()
    </div>
  
            <br /><br /><br /><br /><br />
          <section id="footer" class="footer_widget"  88>
                    <div class="container">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="row">
                                        <div class="col-sm-3 col-xs-12">
                                            <div class="single_widget wow fadeIn" data-wow-duration="800ms">
                                                <div class="footer_logo">
                                                   <img src="~/IndexAssests/images/Screenshot__610_-removebg-preview.png" width="150" />

                                                </div>

                                                <p>&copy;FirstProject By Abdulmalek Musabeh All The Posts Reserved. </p>

                                            </div>
                                     </div>
                                </div>
                            </div>
                        </div>
                    </div>
            </section>
          
        </div>
      
    </div>
   
    <script src="~/DashboardAssests2/plugins/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="~/DashboardAssests2/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/DashboardAssests2/js/app-style-switcher.js"></script>
    <script src="~/DashboardAssests2/plugins/bower_components/jquery-sparkline/jquery.sparkline.min.js"></script>
    <script src="~/DashboardAssests2/js/waves.js"></script>
    <script src="~/DashboardAssests2/js/sidebarmenu.js"></script>
    <script src="~/DashboardAssests2/js/custom.js"></script>
    <script src="~/DashboardAssests2/plugins/bower_components/chartist/dist/chartist.min.js"></script>
    <script src="~/DashboardAssests2/plugins/bower_components/chartist-plugin-tooltips/dist/chartist-plugin-tooltip.min.js"></script>
    <script src="~/DashboardAssests2/js/pages/dashboards/dashboard1.js"></script>
</body>
</html>

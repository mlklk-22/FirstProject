            <td>
                @Html.DisplayFor(modelItem => item.Titlechose1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descriptionchose1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Titlechose2)

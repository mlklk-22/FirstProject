            <td>
                @Html.DisplayFor(modelItem => item.Role.Rolename)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsernameNavigation.Username)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Titleimage">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Titleimage">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Titleimage">Delete</a>
            </td>
        </tr>

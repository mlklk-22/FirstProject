            </th>
            <th>
                @Html.DisplayNameFor(model => model.Title4)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descriptiontitle4)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Titlechose1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descriptionchose1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Titlechose2)

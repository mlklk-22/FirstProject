            <th>
                @Html.DisplayNameFor(model => model.Descriptionchose5)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Choseimage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Role)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsernameNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descriptiontitle1)
            </td>
            <td>
